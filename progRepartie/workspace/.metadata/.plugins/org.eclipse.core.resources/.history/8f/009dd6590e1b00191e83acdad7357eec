package tp1;

import java.util.Stack;

public class PlusieursThreads {
	
	static Stack<Thread> threads;

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		threads = new Stack<>();
		
		Thread t1 = new Thread();
		Thread t2 = new Thread();
		
		threads.push(t1);
		threads.push(t2);
		
		for(int i=0; i<2; i++)
			t1.run(){
			System.out.println("");
		}

	}
	
	public static void creerThread(){
		threads.push(new Thread());
	}

}
