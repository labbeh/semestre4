package tp1;

import java.util.Stack;

public class PlusieursThreads {
	
	static Stack<Thread> threads;

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		threads = new Stack<>();
		
		Thread t1 = new Thread();
		Thread t2 = new Thread();
	}
	
	private class CreerThread1 extends Thread{
		private Thread th1;
		private Thread th2;
		
		public CreerThread1() {
			th1 = new Thread();
			th1 = new Thread();
		}
		
		@Override
		public void run(){
			super.run();
		}
	}

}
