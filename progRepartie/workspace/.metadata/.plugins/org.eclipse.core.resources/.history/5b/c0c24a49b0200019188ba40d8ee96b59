package tp2.prodCons;

public class Lanceur {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Buffer buffer = new Buffer(100);
		
		Producteur 	 prod = new Producteur(buffer);
		Consommateur cons = new Consommateur(buffer);
		
		prod.start();
		cons.start();
		
		prod.join();
		cons.join();
	}

}
